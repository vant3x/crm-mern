{
    "version": 2,
    "builds": [
        {
            "src":"/*.js",
            "use":"@now/node-server"
        }
    ],
    "routes":[
        {
            "src":"/users",
            "dest":"/index.js",
            "methods":["GET","POST","PUT","DELETE"]
        },
        {
            "src":"/{.*}",
            "dest":"/index.js"
        }
    ]
}